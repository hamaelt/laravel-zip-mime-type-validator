{
    "name": "hamaelt/zip-mime-type-validator",
    "version": "1.0.1",
    "description": "Laravel zip file mime type validator",
    "type": "library",
    "keywords": [
        "laravel",
        "validation",
        "mime type",
        "zip",
        "zip validation"
    ],
    "authors": [
        {
            "name": "Hamail Tahir",
            "role": "Developer"
        }
    ] ,
    "require": {
        "php": "^7.4|^8.0",
        "ext-zip": "*",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Hamaelt\\ZipValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hamaelt\\ZipValidator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "release" : [
            "standard-version",
            "git push --follow-tags origin main"
        ]
    }
}

